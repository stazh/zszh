                    <constraintSpec scheme="schematron" module="core" ident="sch-measure" mode="change">
                        <desc>Attributes of tei:measure</desc>
                        <constraint>
                            <sch:pattern>
                                <sch:rule context="tei:measure[matches(@unit,'^(Ballen|Bürde|Dutzend|Fuder|Gaden|Loden|Maschen|Paar|Schubkarre|Seil|Sester|Stock|Stuck|Zeine)$')]">
                                    <sch:assert test="exists(@type)">If @unit is specified on the
                                        <sch:name/> element, @type must be present,
                                        too.</sch:assert>
                                </sch:rule>
                                <sch:rule context="tei:measure[@type='area']">
                                    <sch:assert test="matches(@unit,'^(fauchée|Fuss|Gertel|Hube|Juchart|Kammer|Klafter|Kuhsömmerung|Kuhwinterung|Mal|Mannmad|Mannwerk|Mitmal|Mütt|Rute|Schritt|Schuh|Schuppose|Staffel|Tagwen|Vierdung|Vierling|Zeile)$')">kein gültiges Flächenmass</sch:assert>
                                </sch:rule>
                                <sch:rule context="tei:measure[@type='currency']">
                                    <sch:assert test="matches(@unit,'^(Angster|écu blanc|écu|écu d’or|écu petit|fl|franc|lb|ß|xr|d|Dicken|Doubloon|ducatoon|Dukat|bz|h|M|Mirliton|plp|Böhmischer Groschen|Gros|Grossi|Halbgulden|Halbtaler|k|livre faible|Kronentaler|Louis dor|Louistaler|Pistole|Ort|Quart|Vierer|Spagürli|S|sol faible|Sonnendublone|Taler|L|zecchino)$')">keine gültige Währung</sch:assert>
                                </sch:rule>
                                <sch:rule context="tei:measure[@type='length']">
                                    <sch:assert test="matches(@unit,'^(Daumen|Elle|Finger|Fuss|Klafter|Linie|Meile|Schuh|Stab|Vierling|Zoll)$')">kein gültiges Längenmass</sch:assert>
                                </sch:rule>
                                <sch:rule context="tei:measure[@type='volume']">
                                    <sch:assert test="matches(@unit,'^(Becher|Eimer|Garbe|Immi|Kanne|Kratten|Ledi|Mäss|Malter|Mütt|Sack|Scheffel|Sester|Ster|Vierdung|Vierling|Viertel|Brenten|Fass|Kopf|Klafter|Legel|Mass|Mässli|Quart|Quartane|Saum|Schaffen|Tonne|Viertel|Zuber|Fass|Krötli|Röhrchen|Scheibe|Trinken)$')">kein gültiges Hohlmass</sch:assert>
                                </sch:rule>
                                <sch:rule context="tei:measure[@type='weight']">
                                    <sch:assert test="matches(@unit,'^(Biner|Gewäg|Krinne|Lot|Pfund|Rupp|Quentli|Vierling|Unze|Zentner)$')">kein gültiges Gewicht</sch:assert>
                                </sch:rule>
                                <sch:rule context="tei:measure[@type='age']"> </sch:rule>
                                <sch:rule context="tei:measure[@type='undefined']">
                                    <sch:assert test="matches(@unit,'^(Ballen|Bürde|carriage|Dutzend|Fuder|Gaden|Kloben|Laib|Loden|Mal|Maschen|Paar|Riste|Schoch|Schubkarre|Seil|Sester|Stock|Stuck|Zeine)$')">keine gültige Einheit für ungefähre
                                        Masse/Gewichte</sch:assert>
                                </sch:rule>
                            </sch:pattern>
                        </constraint>
